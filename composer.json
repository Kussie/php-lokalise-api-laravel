{
    "name": "ruben/php-lokalise-api-laravel",
    "description": "Laravel wrapper for the php-lokalise-api package",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ruben Funai",
            "email": "ruben.funai@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "ARG2009\\Lokalise\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ARG2009\\Lokalise\\LokaliseServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.0",
        "lokalise/php-lokalise-api": "^4.0"
    },
    "require-dev": {
        "illuminate/support": "^8.61"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
